module
{
    namespace Elastos {
    namespace Droid {
    namespace Graphics {

    enum PorterDuffMode {
        PorterDuffMode_NONE        = -1,

        /** [0, 0] */
        PorterDuffMode_CLEAR       = 0x00,
        /** [Sa, Sc] */
        PorterDuffMode_SRC         = 0x01,
        /** [Da, Dc] */
        PorterDuffMode_DST         = 0x02,
        /** [Sa + (1 - Sa)*Da, Rc = Sc + (1 - Sa)*Dc] */
        PorterDuffMode_SRC_OVER    = 0x03,
        /** [Sa + (1 - Sa)*Da, Rc = Dc + (1 - Da)*Sc] */
        PorterDuffMode_DST_OVER    = 0x04,
        /** [Sa * Da, Sc * Da] */
        PorterDuffMode_SRC_IN      = 0x05,
        /** [Sa * Da, Sa * Dc] */
        PorterDuffMode_DST_IN      = 0x06,
        /** [Sa * (1 - Da), Sc * (1 - Da)] */
        PorterDuffMode_SRC_OUT     = 0x07,
        /** [Da * (1 - Sa), Dc * (1 - Sa)] */
        PorterDuffMode_DST_OUT     = 0x08,
        /** [Da, Sc * Da + (1 - Sa) * Dc] */
        PorterDuffMode_SRC_ATOP    = 0x09,
        /** [Sa, Sa * Dc + Sc * (1 - Da)] */
        PorterDuffMode_DST_ATOP    = 0x0a,
        /** [Sa + Da - 2 * Sa * Da, Sc * (1 - Da) + (1 - Sa) * Dc] */
        PorterDuffMode_XOR         = 0x0b,
        /** [Sa + Da - Sa*Da,
             Sc*(1 - Da) + Dc*(1 - Sa) + min(Sc, Dc)] */
        PorterDuffMode_DARKEN      = 0x10,
        /** [Sa + Da - Sa*Da,
             Sc*(1 - Da) + Dc*(1 - Sa) + max(Sc, Dc)] */
        PorterDuffMode_LIGHTEN     = 0x11,
        /** [Sa * Da, Sc * Dc] */
        PorterDuffMode_MULTIPLY    = 0x0d,
        /** [Sa + Da - Sa * Da, Sc + Dc - Sc * Dc] */
        PorterDuffMode_SCREEN      = 0x0e,
        /** Saturate(S + D) */
        PorterDuffMode_ADD         = 0x0c,
        PorterDuffMode_OVERLAY     = 0x0f,
    }

    } // namespace Graphics
    } // namespace Droid
    } // namespace Elastos
}
